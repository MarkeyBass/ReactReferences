 be triggered.\r\n  }, [cartCtx.items]);\r\n\r\n  return (\r\n    <button className={btnClasses} onClick={props.onClick}>\r\n      <span className={styles.icon}>\r\n        <CartIcon />\r\n      </span>\r\n      <span className={styles}>Your Cart</span>\r\n      <span className={styles.badge}>{numberOfCartItems}</span>\r\n    </button>\r\n  );\r\n};\r\n\r\nexport default HeaderCartButton;\r\n","C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\Meals\\MealsSummary.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\Cart\\CartItem.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\Meals\\AvailableMeals.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\store\\cart-context.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\Meals\\meals-list.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\UI\\Modal.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\Cart\\CartIcon.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\UI\\Card.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\Meals\\MealItem\\MealItem.js",[],["85","86"],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\Meals\\MealItem\\MealItemForm.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\UI\\Input.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\components\\Cart\\Checkout.js",[],"C:\\dev\\React\\Max\\new\\17_food_order_app_http\\src\\hooks\\useInput.js",[],{"ruleId":"87","severity":1,"message":"88","line":10,"column":11,"nodeType":"89","messageId":"90","endLine":10,"endColumn":16},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},"no-unused-vars","'items' is assigned a value but never used.","Identifier","unusedVar","no-native-reassign",["95"],"no-negated-in-lhs",["96"],"no-global-assign","no-unsafe-negation"]