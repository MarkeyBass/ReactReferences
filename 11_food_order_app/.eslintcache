   <CartIcon />\r\n      </span>\r\n      <span className={styles}>Your Cart</span>\r\n      <span className={styles.badge}>{numberOfCartItems}</span>\r\n    </button>\r\n  );\r\n};\r\n\r\nexport default HeaderCartButton;\r\n","C:\\dev\\React\\Max\\new\\11_food_order_app\\src\\components\\UI\\Modal.js",[],"C:\\dev\\React\\Max\\new\\11_food_order_app\\src\\components\\UI\\Card.js",[],"C:\\dev\\React\\Max\\new\\11_food_order_app\\src\\components\\Cart\\CartIcon.js",[],"C:\\dev\\React\\Max\\new\\11_food_order_app\\src\\components\\Meals\\MealItem\\MealItem.js",[],"C:\\dev\\React\\Max\\new\\11_food_order_app\\src\\components\\Meals\\MealItem\\MealItemForm.js",[],"C:\\dev\\React\\Max\\new\\11_food_order_app\\src\\components\\UI\\Input.js",[],{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":89,"column":9,"nodeType":"87","messageId":"88","endLine":89,"endColumn":20},{"ruleId":"85","severity":1,"message":"89","line":10,"column":11,"nodeType":"87","messageId":"88","endLine":10,"endColumn":16},"no-native-reassign",["90"],"no-negated-in-lhs",["91"],"no-unused-vars","'totalAmount' is assigned a value but never used.","Identifier","unusedVar","'items' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]